  "version": "0.2.0",
  "configurations": [
    {
      "name": "(gdb) Launch",
      "type": "cppdbg",
      "request": "launch",
      "preLaunchTask": "Compile",
      "program": "${workspaceFolder}/project/output/main",
      // <metodo> <quantidade> <situacao> <chave>
      "args": [
        "2",
        "3",
        "1",
        "2"
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}/project",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb"
    }
  ]
}